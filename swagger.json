{
  "swagger": "2.0",
  "info": {
    "description": "Esta es una API de las gasolineras de Mexico, ordenadas por estado, y precio",
    "version": "0.0.1",
    "title": "GasolinasMX"
  },
  "basePath": "/api/stations",
  "tags": [
    {
      "name": "Stations",
      "externalDocs": {
        "description": "",
        "url": "http://swagger.io"
      }
    }
  ],
  "schemes": [
    "https"
  ],
  "paths": {
    "/api/satations": {
      "get": {
        "tags": [
          "Stations"
        ],
        "summary": "Create user",
        "description": "This can only be done by the logged in user.",
        "operationId": "createUser",
        "produces": [
          "application/json"
        ],
        "responses": {
          "default": {
            "description": "successful operation"
          }
        }
      }
    },
    "/user/{station_id}": {
      "get": {
        "tags": [
          "Stations"
        ],
        "summary": "Trae una estacion",
        "description": "De esta forma puedes traer una sola estacion por ID",
        "operationId": "getOneStation",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "station_id",
            "in": "path",
            "description": "The name that needs to be fetched. Use user1 for testing. ",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation"
          },
          "400": {
            "description": "Invalid username supplied"
          },
          "404": {
            "description": "User not found"
          }
        }
      }
    }
  },
  "securityDefinitions": {
    "petstore_auth": {
      "type": "oauth2",
      "authorizationUrl": "http://petstore.swagger.io/oauth/dialog",
      "flow": "implicit",
      "scopes": {
        "write:pets": "modify pets in your account",
        "read:pets": "read your pets"
      }
    },
    "api_key": {
      "type": "apiKey",
      "name": "api_key",
      "in": "header"
    }
  }
}